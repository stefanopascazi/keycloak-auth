    "devDependencies": {
        "prettier": "^2.6.2",
        "ts-loader": "^9.3.0",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^4.7.3",
        "webpack": "^5.73.0",
        "webpack-cli": "^4.9.2"
    },
    "files": [
        "dist"
    ],
